[user]
    email = 
    name = Tushar Sharma
[section]
	key = value
[push]
	default = matching
[color]
	ui = auto
[core]
	eol = crlf
	fscache = true
	trustctime = false
	editor = notepad++.exe -multiInst -nosession \"$(cygpath -m \"$*\")\"
[alias]
	st = status
	ci = commit
	co = checkout
	lg = log --graph --pretty=format:\"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ad) %C(bold blue)<%an>%Creset\" --abbrev-commit --date=relative
[diff]
    tool = winmerge
[difftool "winmerge"]
    cmd = ~/git-difftool-winmerge-wrapper.sh \"$LOCAL\" \"$REMOTE\"
[difftool]
    prompt = false
[merge]
    tool = winmerge
[mergetool "winmerge"]
    cmd = ~/git-difftool-winmerge-wrapper.sh \"$MERGED\" \"$REMOTE\"
[mergetool]
    keepBackup = false
    trustExitCode = false
